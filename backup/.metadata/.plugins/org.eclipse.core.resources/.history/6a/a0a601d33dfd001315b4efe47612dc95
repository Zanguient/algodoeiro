var data = [4, 8, 15, 16, 23, 42];
var init = 0;
var width = 420;
var barHeight = 20;

var dados = d3.tsv

var x = d3.scale.linear().domain([init, d3.max(data)]).range([init, width]);


d3.tsv("data.tsv", function(d) {
		return {
			name:  + d.name,
			value:  + d.value
		};
	}, function (error, rows) {
		console.log(rows);
	});


var chart = d3.select(".chart").attr("width", width).attr("height", barHeight * data.length);

var bar = chart.selectAll("g")// nao entendi esse g
.data(data).enter().append("g")// nao entendi esse append
.attr("transform", function(d, i) {
	return "translate(0," + i * barHeight + ")";
});

bar.append("rect").attr("width", x).attr("height", barHeight - 1);

bar.append("text").attr("x", function(d) {
	return x(d) - 3;
}).attr("y", barHeight / 2).attr("dy", ".35em").text(function(d) {
	return d;
});

/*.data(data)
.enter()
.append("div")
.style("width", function(d) {
return escala(d) + "px";
}).text(function(d) {
return d;
});
